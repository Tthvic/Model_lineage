# Weight Pruning Experiment Configuration
# Copy this file to .env and fill in your paths

# Parent model (model to be pruned)
PARENT_MODEL_PATH=/path/to/Qwen2.5-1.5B-Instruct

# Negative model (unrelated model for comparison)
NEGATIVE_MODEL_PATH=/path/to/Qwen2.5-Math-1.5B

# Lineage model (trained lineage detection model)
LINEAGE_MODEL_PATH=/path/to/lineage_model.pth

# Lineage model code path (for importing TransformerEncoder and VectorRelationNet)
LINEAGE_CODE_PATH=/path/to/lineage_model_code

# GPU ID to use
GPU_ID=0
